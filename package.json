{
  "name": "chiron",
  "version": "1.0.0",
  "description": "A Javascript Testing Framework",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalyvasio/chiron.git"
  },
  "scripts": {
    "test": "chiron --files=/lib/examples/tests",
    "coverage": "tsc && nyc npm run test",
    "build": "tsc",
    "run": "ts-node main.ts"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ]
  },
  "keywords": [
    "unit",
    "testing",
    "testing",
    "development"
  ],
  "bugs": {
    "url": "https://github.com/kalyvasio/chiron/issues"
  },
  "homepage": "https://github.com/kalyvasio/chiron#readme",
  "author": "kalyvasio",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "log-symbols": "^3.0.0",
    "wide-align": "^1.1.3",
    "image-to-ascii": "^3.0.12",
    "typescript-require": "^0.2.10",
    "yargs": "^15.0.2",
    "@types/yargs": "^13.0.0",
    "lodash": "^4.17.15",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.0",
    "babel-cli": "^6.26.0",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.4"
  }
}
